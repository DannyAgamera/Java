<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SquareRooter.SquareRooterController">
    <stylesheets>
        <URL value="@squarerooter.css" />
    </stylesheets>
   <children>
      <VBox layoutY="-14.0" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER" prefHeight="150.0" prefWidth="200.0">
               <children>
                  <Label text="Square Rooter">
                     <font>
                        <Font name="Cambria Bold" size="48.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="150.0" prefWidth="200.0" spacing="50.0">
               <children>
                  <VBox prefHeight="133.0" prefWidth="260.0" spacing="20.0">
                     <children>
                        <TextField fx:id="SquareRootTextField" prefHeight="35.0" prefWidth="260.0" promptText="Enter a squared number">
                           <font>
                              <Font name="Arial Bold" size="14.0" />
                           </font>
                        </TextField>
                        <Button fx:id="SquareRootButton" mnemonicParsing="false" onAction="#squareRootButtonClicked" prefHeight="35.0" prefWidth="260.0" text="Find Square Root">
                           <font>
                              <Font name="Arial Bold" size="18.0" />
                           </font>
                        </Button>
                     </children>
                     <padding>
                        <Insets top="20.0" />
                     </padding>
                  </VBox>
                  <Label fx:id="SquareRootLabel" prefHeight="100.0" prefWidth="200.0" text="Square root" textOverrun="CLIP" wrapText="true">
                     <font>
                        <Font name="Courier New Bold" size="36.0" />
                     </font>
                  </Label>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER" prefHeight="150.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <VBox prefHeight="134.0" prefWidth="250.0" spacing="20.0">
                     <children>
                        <Label fx:id="nextPossibleSquaredNumber" prefHeight="30.0" prefWidth="250.0">
                           <font>
                              <Font name="Arial Bold" size="12.0" />
                           </font>
                        </Label>
                        <Label fx:id="nextPossibleSquaredNumberLabel" prefHeight="30.0" prefWidth="250.0">
                           <font>
                              <Font name="Arial Bold" size="12.0" />
                           </font>
                        </Label>
                     </children>
                     <padding>
                        <Insets top="10.0" />
                     </padding>
                  </VBox>
                  <VBox layoutX="260.0" layoutY="10.0" prefHeight="134.0" prefWidth="250.0" spacing="20.0">
                     <children>
                        <Label fx:id="nextPossibleSquareRoot" prefHeight="30.0" prefWidth="250.0">
                           <font>
                              <Font name="Arial Bold" size="12.0" />
                           </font>
                        </Label>
                        <Label fx:id="nextPossibleSquareRootLabel" prefHeight="30.0" prefWidth="250.0">
                           <font>
                              <Font name="Arial Bold" size="12.0" />
                           </font>
                        </Label>
                     </children>
                     <padding>
                        <Insets top="10.0" />
                     </padding>
                  </VBox>
               </children></HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
